<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_select_picture"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.oa.wanyu.activity.reimbursementActivity.SelectPictureActivity">

    <RelativeLayout
        android:id="@+id/title_select"
        android:layout_width="match_parent"
        android:layout_height="45dp"
        android:background="@color/color_ffffff">

        <ImageView
            android:id="@+id/select_img_back"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_centerVertical="true"
            android:paddingLeft="10dp"
            android:paddingRight="20dp"
            android:src="@mipmap/back_img" />

        <TextView
            android:id="@+id/title_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="10dp"
            android:padding="5dp"
            android:text="完成"
            android:textColor="#000000"
            android:textSize="16sp" />
    </RelativeLayout>

    <GridView
        android:id="@+id/select_img_gridview"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@id/title_select"
        android:numColumns="3">


    </GridView>
</RelativeLayout>
